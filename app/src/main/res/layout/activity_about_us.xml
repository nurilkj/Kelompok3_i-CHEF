            android:layout_width="350dp"
            android:layout_height="6dp"
            android:layout_gravity="center"
            android:layout_marginStart="0dp"
            android:layout_marginTop="0dp"
            android:layout_marginEnd="0dp"
            android:background="#593809" />
        <LinearLayout
            android:id="@+id/orang2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="top"
            android:layout_marginBottom="20dp"
            android:layout_marginTop="20dp"
            android:background="#FFE5BE"
            android:orientation="horizontal">
            <de.hdodenhof.circleimageview.CircleImageView
                android:layout_width="130dp"
                android:layout_marginLeft="16dp"
                android:src="@drawable/nuril"
                android:layout_height="130dp"/>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="top"
                android:layout_marginBottom="20dp"
                android:layout_marginTop="20dp"
                android:background="#FFE5BE"
                android:orientation="vertical">
                <TextView
                    android:layout_gravity="center"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="16dp"
                    android:text="Nuril Khilyatil Jannah"
                    android:textSize="16sp"
                    android:textStyle="bold" />
                <TextView
                    android:layout_gravity="center"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="16dp"
                    android:text="2003421019"
                    android:textSize="16sp" />
                <TextView
                    android:layout_gravity="center"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="16dp"
                    android:text="nuril.khilyatiljannah.te20@mhsw.pnj.ac.id"
                    android:textSize="16sp" />

            </LinearLayout>

        </LinearLayout>
        <View
            android:id="@+id/view3"
            android:layout_width="350dp"
            android:layout_height="6dp"
            android:layout_gravity="center"
            android:layout_marginStart="0dp"
            android:layout_marginTop="0dp"
            android:layout_marginEnd="0dp"
            android:background="#593809" />
        <LinearLayout
            android:id="@+id/orang3"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="top"
            android:layout_marginBottom="20dp"
            android:layout_marginTop="20dp"
            android:background="#FFE5BE"
            android:orientation="horizontal">
            <de.hdodenhof.circleimageview.CircleImageView
                android:layout_width="130dp"
                android:layout_marginLeft="16dp"
                android:src="@drawable/pimpi"
                android:layout_height="130dp"/>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="top"
                android:layout_marginBottom="30dp"
                android:layout_marginTop="20dp"
                android:background="#FFE5BE"
                android:orientation="vertical">
                <TextView
                    android:layout_gravity="center"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="16dp"
                    android:text="Pimpi Haifa Zahra"
                    android:textSize="16sp"
                    android:textStyle="bold" />
                <TextView
                    android:layout_gravity="center"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="16dp"
                    android:text="2003421034"
                    android:textSize="16sp" />
                <TextView
                    android:layout_gravity="center"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="16dp"
                    android:text="pimpi.haifazahra.te20@mhsw.pnj.ac.id"
                    android:textSize="16sp" />
            </LinearLayout>
        </LinearLayout>
        <View
            android:id="@+id/view4"
            android:layout_width="350dp"
            android:layout_height="6dp"
            android:layout_gravity="center"
            android:layout_marginStart="0dp"
            android:layout_marginTop="0dp"
            android:layout_marginEnd="0dp"
            android:background="#593809"/>
    </LinearLayout>
</LinearLayout>